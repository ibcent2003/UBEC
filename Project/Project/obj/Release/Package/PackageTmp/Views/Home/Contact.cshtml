@model Project.Models.IndexViewModel
@{
    ViewBag.Title = "Contact Us";
    Layout = "~/Views/Shared/_PlainLayout.cshtml";
}

<section class="overview-block-ptb iq-over-black-70 jarallax iq-breadcrumb3 text-left iq-font-white" style="background-image: url('/Content/Frontend/revolution/assets/s1.jpg'); background-position: center center; background-repeat: no-repeat; background-size: cover;">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-sm-12">
                <div class="iq-mb-0">
                    <h2 class="iq-font-white iq-tw-6">Contact Us</h2>
                </div>
            </div>
            <div class="col-lg-6 col-sm-12">
                <nav aria-label="breadcrumb" class="text-right">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/"><i class="ion-android-home"></i> Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Contact Us</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</section>


<section class="overview-block-ptb2 green-bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-12">
                <div class="contact-blog text-center iq-font-white iq-mtb-20">
                    <div class="iq-line">
                        <div class="feature-icon">
                            <i aria-hidden="true" class="ion-ios-location-outline"></i>
                        </div>
                        <h4 class="iq-font-black iq-mt-20 iq-mb-10 iq-tw-6">Address</h4>
                        <p>UBEC Building,No 7 Gwani Street, Wuse Zone 4, Abuja, Nigeria</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-12">
                <div class="contact-blog text-center iq-font-white iq-mtb-20">
                    <div class="iq-line">
                        <div class="feature-icon">
                            <i aria-hidden="true" class="ion-ios-telephone-outline"></i>
                        </div>
                        <h4 class="iq-font-black iq-mt-20 iq-mb-10 iq-tw-6">Phone</h4>
                        <span class="iq-lead iq-tw-6">+2349038022457, +2347016390000</span>
                        <p class="iq-mb-0">Mon-Fri 8:00am - 5:00pm</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-12">
                <div class="contact-blog text-center iq-font-white iq-mtb-20">
                    <div class="iq-line">
                        <div class="feature-icon">
                            <i aria-hidden="true" class="ion-ios-email-outline"></i>
                        </div>
                        <h4 class="iq-font-black iq-mt-20 iq-mb-10 iq-tw-6">Email</h4>
                        <span class="iq-lead iq-tw-6">
                            info@ubec.gov.ng
                        </span>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="overview-block-ptb2">
    <div class="container">
        <div class="row">
           
        </div>
    </div>
</section>