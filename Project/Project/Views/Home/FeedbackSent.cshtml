@model Project.Models.IndexViewModel
@{
    ViewBag.Title = "Feedback";
    Layout = "~/Views/Shared/_PlainLayout.cshtml";
}

<section class="overview-block-ptb iq-over-black-70 jarallax iq-breadcrumb3 text-left iq-font-white" style="background-image: url('/Content/Frontend/revolution/assets/s1.jpg'); background-position: center center; background-repeat: no-repeat; background-size: cover;">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-sm-12">
                <div class="iq-mb-0">
                    <h2 class="iq-font-white iq-tw-6">Feedback</h2>
                </div>
            </div>
            <div class="col-lg-6 col-sm-12">
                <nav aria-label="breadcrumb" class="text-right">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/"><i class="ion-android-home"></i> Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Feedback</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</section>


<div class="overview-block-ptb  iq-event-schedule iq-hide">
    <div class="container">
        <div class="row">           
            <div class="col-sm-12">
                <h3 class="small-title iq-tw-6 text-black">Message Sent</h3>



                <div class="iq-mt-30">
                    <div class="alert alert-success" role="alert">
                        <h4 class="alert-heading">Well done!</h4>
                        <p>Your message has been submitted successfully. One of our representative would contact you within 5 working day.</p>
                        <hr>
                        <p class="mb-0">Thank you.</p>
                    </div>
                </div>
            </div>

          


        </div>




    </div>
</div>