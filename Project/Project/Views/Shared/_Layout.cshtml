    <script src='https://www.google.com/recaptcha/api.js'></script>
    <!-- Mega Menu -->
    <script src="~/Content/Frontend/js/mega-menu/mega_menu.js"></script>
    <!-- Main -->
    <script src="~/Content/Frontend/js/main.js"></script>
    <!-- ADD-ONS JS FILES -->
    <script src="~/Content/Frontend/revolution/js/revolution.addon.particles.min.js"></script>
    <!-- REVOLUTION JS FILES -->
    <script src="~/Content/Frontend/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/Content/Frontend/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <!-- SLIDER REVOLUTION 5.0 EXTENSIONS  (Load Extensions only on Local File Systems !  The following part can be removed on Server for On Demand Loading) -->
